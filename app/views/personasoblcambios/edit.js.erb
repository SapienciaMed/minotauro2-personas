$("#personasoblcambio_form").collapse("hide");

$("#personasoblcambio_form").empty();

<% if @active_record.present? %>
	$("#personasoblcambio_<%= @active_record.id %>").replaceWith("<%= j render 'tr', personasoblcambio: @active_record %>");
<% end %>

$("#personasoblcambio_<%= @personasoblcambio.id %>").html("<td colspan='7'><%= j render 'personasoblcambios_form' %></td>");

$("#personasoblcambio_<%= @personasoblcambio.id %>").addClass("active_personasoblcambio");

$(".select2").select2();

$(".tooltip").tooltip("hide");

$('[data-toggle="tooltip"]').tooltip();
