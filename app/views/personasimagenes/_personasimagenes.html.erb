<div class="row">
  <div class="col-md-12">
    <div class="box box-primary">
      <div class="box-header">
        <legend>Digital - Documentos</legend>
        <div class="box-tools pull-right">
          <% if is_auth_c('personasimagen') %>
            <%= link_to new_persona_personasimagen_path(persona_id: @persona.id), remote: true, class: 'btn btn-sm btn-success' do %>
              <i class="fa fa-plus-square"> </i> <span><%= t :agregar %></span>
            <% end %>
          <% end %>
        </div>
        <div id="personasimagen_form" class="collapse"></div>
      </div>
      <div class="box-body">
        <div id="personasimagenes">
          <% if @persona.personasimagenes.present? %>
            <%= render 'personasimagenes/tabla' %>
          <% else %>
            <h4 class="text-center">No hay datos asociados</h4>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="modal fade" id="newModal">
  <div class="modal-dialog">
    <div class="modal-content">
    </div>
  </div>
</div>
