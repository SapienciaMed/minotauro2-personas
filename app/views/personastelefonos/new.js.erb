<% if @active_record.present? %>
	$("#personastelefono_<%= @active_record.id %>").replaceWith("<%= j render 'tr', personastelefono: @active_record %>");
<% end %>
$("#personastelefono_form").html("<%= j render 'form' %>");
$(".datetimepicker").datetimepicker({
	showClear: true
});
$(".tooltip").tooltip("hide");
$('[data-toggle="tooltip"]').tooltip();
$("#personastelefono_form").collapse("show");
$(".select2").select2();
