<% if @active_record.present? %>
	$("#personasdireccion_<%= @active_record.id %>").replaceWith("<%= j render 'tr', personasdireccion: @active_record %>");
<% end %>
$("#personasdireccion_form").html("<%= j render 'form' %>");
$(".datetimepicker").datetimepicker({
	showClear: true
});
$(".tooltip").tooltip("hide");
$('[data-toggle="tooltip"]').tooltip();
$("#personasdireccion_form").collapse("show");
$(".select2").select2();
